{
  "namespace": "tabbed_upsell",

  "jsonui_warning_dlga@ir2.dlga": {
    "$toggle_name": "jsonui_warning"
  },
  "jsonui_warning_dlga_c@ir2.dlga_c": {
    "$toggle_name": "jsonui_warning"
  },
  "jsonui_warning_dlga_close@ir2.close_dlga_esc": {
    "$toggle_name": "jsonui_warning"
  },

  "warning_icon": {
    "type": "image",
    "texture": "textures/ui/ErrorGlyph",
    "layer": 3,
    "size": [ 13, 20 ]
  },



  "indexs_panel": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ]
  },

  "version_toggle_button_content": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "labels": {
          "type": "stack_panel",
          "size": [ "100%", 10 ],
          "offset": [ 2, 0 ],
          "orientation": "horizontal",
          "controls": [
            {
              "gotesupporter": {
                "type": "label",
                "text": "Gote-Supporter ",
                "color": "$text_color"
              }
            },
            {
              "versions": {
                "type": "label",
                "text": "$version",
                "color": "$text_color"
              }
            }
          ]
        }
      },
      {
        "dropdown_chevron": {
          "type": "image",
          "texture": "textures/ui/dropdown_chevron",
          "anchor_from": "right_middle",
          "anchor_to": "right_middle",
          "offset": [ -5, 0 ],
          "size": [ 11, 11 ]
        }
      }
    ]
  },

  "version_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "$version|default": "v1.0.0",
    "$details_controls|default": [],
    "controls": [
      {
        "toggle_button@common_toggles.light_content_toggle": {
          "size": [ "100%", 20 ],
          "$button_content": "tabbed_upsell.version_toggle_button_content",
          "$toggle_name": "$version",
          "$toggle_view_binding_name": "$version"
        }
      },
      {
        "padding_1": {
          "type": "panel",
          "size": [ 0, 3 ]
        }
      },
      {
        "details_panel": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "controls": "$details_controls",
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "$version",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 0, 3 ]
        }
      }
    ]
  },

  "text": {
    "type": "label",
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "size": [ "100%", "default" ],
    "shadow": true
  },
  
  "playsound_play": {
    "type": "image",
    "texture": "textures/ui/playsound_play",
    "layer": 3,
    "size": [ 16, 16 ]
  },
  
  "playsound_stop": {
    "type": "image",
    "texture": "textures/ui/playsound_stop",
    "layer": 3,
    "size": [ 16, 16 ]
  },
  
  "play_sound_panel": {
    "type": "stack_panel", 
    "size": [ "100%", "100%c + 3px" ],
    "$sound_name|defalut": "dummy",
    "$sound_id|defalut": "pvp2.music.daisuke",
    "$sound_vol|defalut": 1.0,
    "$is_music|defalut": false,
    "controls": [
      {
        "music_name_label": {
          "type": "label",
          "color": "$main_header_text_color",
          "size": [ "100%", "default" ],
          "text": "$sound_name",
          "shadow": true
        }
      },
      {
        "play_stop": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ 61, 25 ],
          "controls": [
            {
              "play_button@common_buttons.light_content_button": {
                "size": [ 25, 25 ],
                "$button_content": "tabbed_upsell.playsound_play",
                "sound_name": "$sound_id",
                "sound_volume": "$sound_vol",
                "$pressed_button_name": "button.dummy"
              }
            },
            {
              "padding": {
                "type": "panel",
                "size": [ 1, 0 ]
              }
            },
            {
              "stop_button@common_buttons.light_content_button": {
                "size": [ 25, 25 ],
                "ignored": "(not $is_music)",
                "$button_content": "tabbed_upsell.playsound_stop",
                "sound_name": "$sound_id",
                "sound_volume": 0.00000001,
                "$pressed_button_name": "button.dummy"
              }
            }
          ]
        }
      }
    ]
  },
  
  "dlga@common_toggles.light_text_toggle": {
    "$radio_toggle_group": true,
    "$toggle_focus_enabled": true,
    "$binding_type": "global",
    "$binding_condition": "once"
  },

