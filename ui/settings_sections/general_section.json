{
  "namespace": "general_section",

  "playsound_play": {
    "type": "image",
    "texture": "textures/ui/playsound_play",
    "layer": 3,
    "size": [ 16, 16 ]
  },
  
  "playsound_stop": {
    "type": "image",
    "texture": "textures/ui/playsound_stop",
    "layer": 3,
    "size": [ 16, 16 ]
  },
  
  "play_sound_panel": {
    "type": "panel",
    "size": [ "100%", 32 ],
    "controls": [
      {
        "sound_panel": {
          "type": "image", 
          "size": [ "100%", 30 ],
          "texture": "textures/ui/White",
          "color": [ 0.1, 0.1, 0.1 ],
          "$sound_name|defalut": "dummy",
          "$sound_id|defalut": "pvp2.music.daisuke",
          "$sound_vol|defalut": 1.0,
          "$is_music|defalut": false,
          "controls": [
            {
              "flame": {
                "type": "image",
                "texture": "textures/ui/focus_border_white",
                "size": [ "100%", "100%" ],
                "color": [ 0, 0, 0 ]
              }
            },
            {
              "panel": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100% - 6px", 10 ],
                "layer": 1,
                "controls": [
                  {
                    "onpu": {
                      "type": "image",
                      "texture": "textures/ui/onpu",
                      "size": [ 10, 10 ]
                    }
                  },
                  {
                    "padding": {
                      "type": "panel",
                      "size": [ 2, 0 ]
                    }
                  },
                  {
                    "sound_name": {
                      "type": "label",
                      "size": [ "fill", 10 ],
                      "text": "$sound_name"
                    }
                  }
                ]
              }
            },
            {
              "play_stop": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100%c + 3px", 25 ],
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "controls": [
                  {
                    "play_button@common_buttons.dark_content_button": {
                      "size": [ 25, 25 ],
                      "$button_content": "general_section.playsound_play",
                      "sound_name": "$sound_id",
                      "sound_volume": "$sound_vol",
                      "$pressed_button_name": "button.dummy"
                    }
                  },
                  {
                    "padding": {
                      "type": "panel",
                      "ignored": "(not $is_music)",
                      "size": [ 1, 0 ]
                    }
                  },
                  {
                    "stop_button@common_buttons.dark_content_button": {
                      "size": [ 25, 25 ],
                      "ignored": "(not $is_music)",
                      "$button_content": "general_section.playsound_stop",
                      "sound_name": "$sound_id",
                      "sound_volume": 0.00000001,
                      "$pressed_button_name": "button.dummy"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "gamertag_controls/sign_out": {
    "ignored": "(not $is_pregame or $requires_xbl_signin_to_play)"
